{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/_admin/**",
        "headers": [
          {
            "key": "X-Robots-Tag",
            "value": "noindex, nofollow"
          }
        ]
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "testlogin-4767c",
          "appId": "1:528531549397:android:556cc048d7fc8360eec65c",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "testlogin-4767c",
          "configurations": {
            "android": "1:528531549397:android:556cc048d7fc8360eec65c",
            "ios": "1:528531549397:ios:dc36af644a6f944ceec65c",
            "web": "1:528531549397:web:50ec91988b8bc6beeec65c"
          }
        }
      }
    }
  }
}
